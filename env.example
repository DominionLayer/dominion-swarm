# Dominion Configuration
# Copy this file to .env and fill in your values

# LLM Provider (openai | anthropic | stub)
LLM_PROVIDER=stub

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview

# Anthropic Configuration
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# EVM Configuration
EVM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-api-key
EVM_CHAIN_ID=1

# Executor Configuration (DANGEROUS - only set if you know what you're doing)
# EXECUTOR_PRIVATE_KEY=0x...
# EXECUTOR_ENABLED=false

# Database
DATABASE_PATH=./data/dominion.db

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Rate Limiting
RPC_RATE_LIMIT=10
RPC_RATE_INTERVAL=1000

# Worker Pool
WORKER_CONCURRENCY=4

# Reports
REPORTS_DIR=./reports


